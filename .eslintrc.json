{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "linebreak-style": 0,
      "object-curly-newline": 0
    }
}
